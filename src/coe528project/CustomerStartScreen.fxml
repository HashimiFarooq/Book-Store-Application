<AnchorPane id="AnchorPane" prefHeight="535.0" prefWidth="507.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="coe528project.CustomerStartScreen">
   <children>
      <TableView fx:id="tableView" layoutX="-1.0" layoutY="74.0" prefHeight="300.0" prefWidth="508.0">
        <columns>
          <TableColumn fx:id="bookName" prefWidth="215.0" text="Book Name" />
          <TableColumn fx:id="bookPrice" minWidth="0.0" prefWidth="214.0" text="Book Price" />
            <TableColumn fx:id="selectBook" prefWidth="78.0" text="Select" />
        </columns>
      </TableView>
      <Button fx:id="logoutButton" layoutX="348.0" layoutY="465.0" mnemonicParsing="false" onAction="#userLogout" prefHeight="39.0" prefWidth="124.0" text="Logout">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="buyButton" layoutX="34.0" layoutY="396.0" mnemonicParsing="false" onAction="#buyBook" prefHeight="39.0" prefWidth="124.0" text="Buy">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="redeemPointsAndBuyButton" layoutX="34.0" layoutY="465.0" mnemonicParsing="false" onAction="#redeemPointsAndBuyBook" prefHeight="39.0" prefWidth="213.0" text="Redeem Points and Buy">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="welcomeMessage" layoutX="19.0" layoutY="12.0" prefHeight="53.0" prefWidth="470.0" />
   </children>
</AnchorPane>
